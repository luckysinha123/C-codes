#include <stdio.h>

int main() {
int n,g,sum1 = 0,S, sum = 0;

scanf("%d %d", &n,&g);
int arr[n], arr1[g];

// input for first array 

for(int i =0; i<n; i++){
    scanf("%d",&arr[i]);
}

// sum of first array 

for (int i=0; i<n; i++){
    sum =sum +arr[i];
}

// input for sec array

for(int i =0; i<g; i++){
    scanf("%d",&arr1[i]);
}

//sum of sec array 

for (int i=0; i<g; i++){
    sum1 =sum1 +arr[i];
}

//Total sum of both the arrays 

S = sum +sum1; 

printf("%d",S);

return 0;
}
